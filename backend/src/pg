db() { PGPASSWORD=123 psql -U impresaone -h postgres_dev impresaone_$@; }
kt() { killall -9 tail; }
t() { tail -f /var/log/impresaone/debugging.log /var/log/impresaone/workers.log /var/log/impresaone/impresaone.services.services.log & }

order() { PYTHONPATH=/app /usr/local/bin/python /app/workers/order_fetch_info.py -c $@; }
